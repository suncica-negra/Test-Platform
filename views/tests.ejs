            <% var minutes = "0" + test.date.getMinutes() %>
            <% var dateString = day + "." +(month + 1) + "." + year + " " + hours + ":" + minutes.substr(-2) %>
            <tr>
               <td><small><%= dateString %></small></td>
               <td><small><%= test.username %></small></td>

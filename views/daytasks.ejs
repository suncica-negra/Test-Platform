<%- include('partials/header1') %>
<div class="container">
   <div class="row mb-3">
      <%- include("partials/menu1"); -%>
      <div class="col-md-8 themed-grid-col data">
         <h2>Baza korisnika / učenika:</h2>
         <table>
            <tr>
               <th>Učenik</th>
               <th>Odgovor</th>
               <th>Kod</th>
               <th>Datum spremanja</th>
            </tr>
            <%  daytasks.forEach(function(daytask){ %>
            <tr>
               <td><%= daytask.username %></td>
               <td><%= daytask.daytask %></td>
               <td><%= daytask.kod %></td>
               <td><%= daytask.date.toLocaleString() %></td>
            </tr>
            <% }) %>
         </table>
      </div>
   </div>
</div>
<%- include("partials/footer"); -%>